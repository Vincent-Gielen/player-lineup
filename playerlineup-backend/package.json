    "config": "^3.3.12",
    "helmet": "^8.0.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "koa": "^2.15.3",
    "koa-bodyparser": "^4.4.1",
    "koa-helmet": "^8.0.1",
    "koa2-swagger-ui": "^5.11.0",
    "prisma": "^5.21.1",
    "react-hook-form": "^7.53.2",
    "swagger-jsdoc": "^6.2.8",
    "winston": "^3.14.1"
