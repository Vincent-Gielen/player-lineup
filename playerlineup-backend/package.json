    "apidoc": "^1.2.0",
    "env-cmd": "^10.1.0",
    "eslint": "^9.12.0",
    "eslint-plugin-jest": "^28.9.0",
    "grunt-apidoc": "^0.11.0",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
